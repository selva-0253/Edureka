pipeline {
    agent any
    environment{
	PATH = "/usr/bin:${env.PATH}"
	}
	stages{
        stage('Run Ansible Playbook')
	{
	   steps{
            sshagent(credentials: ['ansible-key']) {
	     
                sh 'ansible-playbook -i hosts /home/ec2-user/edureka/ansible/install_docker.yaml'
            }
        }
    }
}
}
